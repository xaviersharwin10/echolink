{
  "name": "echolink-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "node dist/index.js",
    "metta-poc": "ts-node src/poc/metta_poc.ts",
    "agent": "cd src/poc && ./start_agent.sh",
    "agent:install": "cd src/poc && ./venv/bin/pip install -r requirements.txt",
    "intelligent": "cd src/poc && ./start_intelligent.sh",
    "intelligent:proxy": "nodemon --exec ts-node src/poc/intelligent_proxy.ts",
    "ingest": "cd src/poc && source venv/bin/activate && python ingest.py",
    "creator-studio": "nodemon --exec ts-node src/creator-studio-server.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@huggingface/inference": "^4.11.1",
    "@huggingface/transformers": "^3.7.5",
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.78",
    "@langchain/openai": "^0.6.14",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "faiss-node": "^0.5.1",
    "fs-extra": "^11.2.0",
    "langchain": "^0.3.35",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "pdf-parse": "^1.1.1",
    "python-shell": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.7.0",
    "@types/pdf-parse": "^1.1.4",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
